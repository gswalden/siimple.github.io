  <!-- Panels -->
  <a name="panels"><h2># Panels</h2></a>

  <!-- Default panels -->
  A panel consists in one <code>div</code> with the class <code>.panel</code>, and two <code>div</code> inside,
  the first with the class <code>.panel-head</code> and the second with the class <code>.panel-body</code>.

  <!-- Default panels demo -->
  <div class="box">
    <div class="panel">
      <div class="panel-head">Panel</div>
      <div class="panel-body">This is the content of the panel</div>
    </div>
  </div>

  <pre>&lt;div class="panel"&gt;<br>&lt;div class="panel-head"&gt;Panel&lt;/div&gt;<br>&lt;div class="panel-body"&gt;This is the content of the panel&lt;/div&gt;<br>&lt;/div&gt;</pre>


  <!-- Spaces -->
  <br>

  <!-- Tables -->
  <a name="tables"><h2># Tables</h2></a>

  <!-- Basic table -->
  <h4>Basic table</h4>
  Simply add <code>.table</code> to the class of your table.

  <!-- Basic table demo -->
  <div class="box">
    <table class="table">
      <thead>
        <tr>
          <td>#</td>
          <td>Col1</td>
          <td>Col2</td>
          <td>Col3</td>
        </tr>
      </thead>
      <tr>
        <td>Row1</td>
        <td>Item</td>
        <td>Item</td>
        <td>Item</td>
      </tr>
      <tr>
        <td>Row2</td>
        <td>Item</td>
        <td>Item</td>
        <td>Item</td>
      </tr>
    </table>
  </div>

  <!-- Basic table code -->
  <pre>&lt;table class="table"&gt; ... &lt;/table&gt;</pre>


  <!-- Hover table -->
  <h4>Hover table</h4>
  Add <code>.table-hover</code> to the class of your table.

  <!-- Hover table demo -->
  <div class="box">
    <table class="table table-hover">
      <thead>
        <tr>
          <td>#</td>
          <td>Col1</td>
          <td>Col2</td>
          <td>Col3</td>
        </tr>
      </thead>
      <tr>
        <td>Row1</td>
        <td>Item</td>
        <td>Item</td>
        <td>Item</td>
      </tr>
      <tr>
        <td>Row2</td>
        <td>Item</td>
        <td>Item</td>
        <td>Item</td>
      </tr>
    </table>
  </div>

  <!-- Hover table code -->
  <pre>&lt;table class="table table-hover"&gt; ... &lt;/table&gt;</pre>


  <!-- Striped table -->
  <h4>Striped rows table</h4>
  Add <code>.table-striped</code> to the class of your table.

  <!-- Basic table demo -->
  <div class="box">
    <table class="table table-striped">
      <thead>
        <tr>
          <td>#</td>
          <td>Col1</td>
          <td>Col2</td>
          <td>Col3</td>
        </tr>
      </thead>
      <tr>
        <td>Row1</td>
        <td>Item</td>
        <td>Item</td>
        <td>Item</td>
      </tr>
      <tr>
        <td>Row2</td>
        <td>Item</td>
        <td>Item</td>
        <td>Item</td>
      </tr>
      <tr>
        <td>Row3</td>
        <td>Item</td>
        <td>Item</td>
        <td>Item</td>
      </tr>
      <tr>
        <td>Row4</td>
        <td>Item</td>
        <td>Item</td>
        <td>Item</td>
      </tr>
    </table>
  </div>

  <!-- Striped table code -->
  <pre>&lt;table class="table table-striped"&gt; ... &lt;/table&gt;</pre>


  <!-- Bordered table -->
  <h4>Bordered table</h4>
  Add <code>.table-border</code> to the class of your table.

  <!-- Bordered table demo -->
  <div class="box">
    <table class="table table-border">
      <thead>
        <tr>
          <td>#</td>
          <td>Col1</td>
          <td>Col2</td>
          <td>Col3</td>
        </tr>
      </thead>
      <tr>
        <td>Row1</td>
        <td>Item</td>
        <td>Item</td>
        <td>Item</td>
      </tr>
      <tr>
        <td>Row2</td>
        <td>Item</td>
        <td>Item</td>
        <td>Item</td>
      </tr>
    </table>
  </div>

  <!-- Bordered table code -->
  <pre>&lt;table class="table table-border"&gt; ... &lt;/table&gt;</pre>

  <p>Additionally you can add the colum border adding <code>.table-border-colum</code>.

  <!-- Bordered column table demo -->
  <div class="box">
    <table class="table table-border-column">
      <thead>
        <tr>
          <td>#</td>
          <td>Col1</td>
          <td>Col2</td>
          <td>Col3</td>
        </tr>
      </thead>
      <tr>
        <td>Row1</td>
        <td>Item</td>
        <td>Item</td>
        <td>Item</td>
      </tr>
      <tr>
        <td>Row2</td>
        <td>Item</td>
        <td>Item</td>
        <td>Item</td>
      </tr>
    </table>
  </div>

  <!-- Bordered column table code -->
  <pre>&lt;table class="table table-border-column"&gt; ... &lt;/table&gt;</pre>

